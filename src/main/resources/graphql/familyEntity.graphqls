type FamilyEntity{
id:Long
surname:String
name:String
fathername:String
birthday:LocalDate
}

type Query{
findAllFamilies: [FamilyEntity]
countFamilies: Long
}

type Mutation{
createFamily(surname:String, name:String, fathername:String, birthday:LocalDate):FamilyEntity
updateFamily(id:Long, surname:String, name:String, fathername:String, birthday:LocalDate):FamilyEntity
deleteFamily(id:Long):Boolean
}